        assert(((cast(*void)(*instance.redBits)) - cast(*void)(*instance)) == 8, "GLFWvidmode.redBits has unexpected offset % instead of 8", ((cast(*void)(*instance.redBits)) - cast(*void)(*instance)));
        assert(size_of(type_of(GLFWvidmode.redBits)) == 4, "GLFWvidmode.redBits has unexpected size % instead of 4", size_of(type_of(GLFWvidmode.redBits)));
        assert(((cast(*void)(*instance.greenBits)) - cast(*void)(*instance)) == 12, "GLFWvidmode.greenBits has unexpected offset % instead of 12", ((cast(*void)(*instance.greenBits)) - cast(*void)(*instance)));
