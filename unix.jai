        assert(((cast(*void)(*instance.refreshRate)) - cast(*void)(*instance)) == 20, "GLFWvidmode.refreshRate has unexpected offset % instead of 20", ((cast(*void)(*instance.refreshRate)) - cast(*void)(*instance)));
        assert(size_of(type_of(GLFWvidmode.refreshRate)) == 4, "GLFWvidmode.refreshRate has unexpected size % instead of 4", size_of(type_of(GLFWvidmode.refreshRate)));
        assert(size_of(GLFWvidmode) == 24, "GLFWvidmode has size % instead of 24", size_of(GLFWvidmode));
